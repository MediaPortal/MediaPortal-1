		<control>
			<description>TV background</description>
			<type>image</type>
			<id>1</id>
			<posX>230</posX>
			<posY>65</posY>
			<width>505</width>
			<height>450</height>
			<texture>-</texture>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>video window</description>
			<type>videowindow</type>
			<id>99</id>
			<posX>240</posX>
			<posY>190</posY>
			<width>396</width>
			<height>228</height>
			<onleft>2</onleft>
			<onright>2</onright>
			<action>18</action>
			<textureFocus>tv_green_border.png</textureFocus>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>Current program title</description>
			<type>fadelabel</type>
			<id>13</id>
			<posX>328</posX>
			<posY>158</posY>
			<width>240</width>
			<height>20</height>
			<label>#TV.View.title</label>
			<font>font13</font>
			<align>center</align>
			<textcolor>White</textcolor>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>Current program time</description>
			<type>label</type>
			<id>13</id>
			<posX>370</posX>
			<posY>435</posY>
			<label>#TV.View.start-#TV.View.stop</label>
			<font>font13</font>
			<align>right</align>
			<textcolor>FFB2D4F5</textcolor>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>Current Channelname</description>
			<type>fadelabel</type>
			<id>13</id>
			<posX>335</posX>
			<posY>128</posY>
			<width>200</width>
			<label>#TV.View.channel</label>
			<font>font13</font>
			<align>center</align>
			<textcolor>white</textcolor>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>Description1</description>
			<type>textboxscrollup</type>
			<id>15</id>
			<posX>290</posX>
			<posY>458</posY>
			<width>298</width>
			<height>47</height>
			<seperator>----------------------------------------------------------------------------------------------</seperator>
			<label>#TV.View.description</label>
			<font>font13</font>
			<textcolor>White</textcolor>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>progress bar</description>
			<type>progress</type>
			<id>20</id>
			<posX>375</posX>
			<posY>439</posY>
			<width>200</width>
			<height>15</height>
			<label>#TV.View.Percentage</label>
			<visible>yes</visible>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>rectangle</description>
			<type>image</type>
			<id>21</id>
			<posX>55</posX>
			<posY>448</posY>
			<width>90</width>
			<height>90</height>
			<texture>tv_background.png</texture>
			<colorkey>0</colorkey>
			<colordiffuse>White</colordiffuse>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<type>image</type>
			<description>title image</description>
			<id>1</id>
			<posX>385</posX>
			<posY>440</posY>
			<width>180</width>
			<texture>progress_over.png</texture>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>Recording Channel</description>
			<type>image</type>
			<id>22</id>
			<posX>60</posX>
			<posY>453</posY>
			<width>80</width>
			<height>80</height>
			<texture>#TV.Record.thumb</texture>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="500">WindowClose</animation>
		</control>
		<control>
			<description>record info label</description>
			<type>fadelabel</type>
			<id>23</id>
			<width>150</width>
			<height>30</height>
			<posX>85</posX>
			<posY>530</posY>
			<label>#TV.Record.channel\r#TV.Record.title\r#TV.Record.start-#TV.Record.stop</label>
