<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <type>image</type>
      <posX>1250</posX>
      <posY>15</posY>
      <width>35</width>
      <height>35</height>
      <texture>Clock.png</texture>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
    </control>
    <control>
      <description>Date</description>
      <type>label</type>
      <id>200</id>
      <posX>1230</posX>
      <posY>30</posY>
      <label>#date</label>
      <align>right</align>
      <font>font12</font>
      <textcolor>ff969696</textcolor>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
    </control>
    <control>
      <description>Time</description>
      <type>label</type>
      <id>201</id>
      <posX>1233</posX>
      <posY>10</posY>
      <label>#time</label>
      <align>right</align>
      <font>font12</font>
      <textcolor>ff969696</textcolor>
      <animation effect="fade" time="200">WindowOpen</animation>
      <animation effect="fade" time="200">WindowClose</animation>
    </control>
  </controls>
</window>