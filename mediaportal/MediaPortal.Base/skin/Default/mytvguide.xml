      <type>spincontrol</type>
      <id>6</id>
      <posX>29</posX>
      <posY>125</posY>
      <width>29</width>
      <height>27</height>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <onup>21</onup>
      <onleft>60</onleft>
      <onright>8</onright>
