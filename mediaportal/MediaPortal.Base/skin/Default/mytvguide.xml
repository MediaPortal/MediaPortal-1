      <posX>0</posX>
      <posY>1</posY>
      <width>25</width>
      <height>34</height>
      <zoom>no</zoom>
      <keepaspectratio>yes</keepaspectratio>
      <texture>defaultVideoBig.png</texture>
    </control>
    <control>
