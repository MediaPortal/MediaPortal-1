      <iconAlign>right</iconAlign>
      <iconVAlign>middle</iconVAlign>
      <iconOffsetX>0</iconOffsetX>
      <iconOffsetY>3</iconOffsetY>
      <iconInlineLabel1>no</iconInlineLabel1>
