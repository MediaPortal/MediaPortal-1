<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>7890</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#selecteditem:#Play.Current.Title  |  #Play.Current.Artist  |  #Play.Current.Album</define>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <texture>BH Radio.jpg</texture>
      <posX>-160</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>720</height>
    </control>
    <import>common.music.play.fanart.xml</import>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <control>
      <type>label</type>
      <description>stream label</description>
      <id>0</id>
      <posX>22</posX>
      <posY>30</posY>
      <width>533</width>
      <label>#Play.Current.Lastfm.CurrentStream</label>
      <font>font11</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <description>General Overlay</description>
      <id>0</id>
      <type>image</type>
      <posX>0</posX>
      <posY>30</posY>
      <width>960</width>
      <height>720</height>
      <texture>general_overlay.png</texture>
    </control>
    <control>
      <type>image</type>
      <description>artist art image</description>
      <id>112</id>
      <posX>708</posX>
      <posY>245</posY>
      <width>195</width>
      <height>195</height>
      <texture mask="cdthumbmask.png">#Play.Current.ArtistThumb</texture>
      <centered>yes</centered>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <type>image</type>
      <description>artist art image</description>
      <id>0</id>
      <posX>708</posX>
      <posY>245</posY>
      <width>195</width>
      <height>195</height>
      <texture mask="cdthumbmask.png">cdthumbframe.png</texture>
      <centered>yes</centered>
      <visible>player.hasmedia</visible>
    </control>
    <control>
      <description>group element</description>
      <type>group</type>
      <layout>StackLayout</layout>
      <posX>37</posX>
      <posY>180</posY>
      <control>
        <description>start a stream</description>
        <type>button</type>
        <id>10</id>
        <posX>79</posX>
        <posY>137</posY>
        <label>34001</label>
        <onleft>10</onleft>
        <onright>123</onright>
        <onup>666</onup>
        <ondown>15</ondown>
      </control>
      <control>
        <description>choose artist</description>
        <type>button</type>
        <id>15</id>
        <label>Artists</label>
        <onleft>15</onleft>
        <onright>123</onright>
        <onup>10</onup>
        <ondown>20</ondown>
      </control>
      <control>
        <description>choose tag</description>
        <type>button</type>
        <id>20</id>
        <label>Tags</label>
        <onleft>20</onleft>
        <onright>123</onright>
        <onup>15</onup>
        <ondown>30</ondown>
      </control>
      <control>
        <description>choose friend</description>
        <type>button</type>
        <id>30</id>
        <label>Friends</label>
        <onleft>30</onleft>
        <onright>123</onright>
        <onup>20</onup>
        <ondown>35</ondown>
      </control>
      <control>
        <description>toggle submit mode</description>
        <type>togglebutton</type>
        <id>35</id>
        <label>34014</label>
        <onleft>35</onleft>
        <onright>123</onright>
        <onup>30</onup>
        <ondown>40</ondown>
        <visible>true</visible>
      </control>
      <control>
        <description>toggle discovery mode</description>
        <type>togglebutton</type>
        <id>40</id>
        <label>34013</label>
        <onleft>40</onleft>
        <onright>123</onright>
        <onup>35</onup>
        <ondown>10</ondown>
        <visible>false</visible>
      </control>
    </control>
    <control>
      <type>image</type>
      <id>117</id>
      <posX>711</posX>
      <posY>452</posY>
      <width>190</width>
      <height>18</height>
      <texture>osd_progress_background_medium.png</texture>
      <visible>Player.HasAudio+!Player.HasVideo+string.equals(#duration)</visible>
    </control>
    <control>
      <description>progress bar</description>
      <type>progress</type>
      <id>118</id>
      <posX>701</posX>
      <posY>453</posY>
      <width>205</width>
      <height>16</height>
      <label>#percentage</label>
      <texturebg>-</texturebg>
      <onlymidtexture>no</onlymidtexture>
      <midwidth>205</midwidth>
      <midheight>16</midheight>
      <midoffsetX>0</midoffsetX>
      <midoffsetY>0</midoffsetY>
      <lefttexture>osd_progress_left.png</lefttexture>
      <midtexture>osd_progress_mid.png</midtexture>
      <righttexture>osd_progress_right.png</righttexture>
      <visible>Player.HasAudio+!Player.HasVideo+string.equals(#duration)</visible>
    </control>
    <control>
      <type>label</type>
      <description>track progress label</description>
      <id>119</id>
      <posX>704</posX>
      <posY>445</posY>
      <label>#currentplaytime</label>
      <font>font14</font>
      <align>right</align>
      <textcolor>ivory</textcolor>
      <visible>Player.HasAudio+!Player.HasVideo+string.equals(#duration)</visible>
    </control>
    <control>
      <type>label</type>
      <description>track duration label</description>
      <id>110</id>
      <posX>909</posX>
      <posY>445</posY>
      <label>#currentremaining</label>
      <font>font14</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
      <visible>Player.HasAudio+!Player.HasVideo+string.equals(#duration)</visible>
    </control>
    <control>
      <description>Seperator</description>
      <type>image</type>
      <id>1</id>
      <posX>60</posX>
      <posY>515</posY>
      <width>855</width>
      <height>1</height>
      <texture>hbar1white.png</texture>
      <colordiffuse>60ffffff</colordiffuse>
    </control>
    <control>
      <type>label</type>
      <description>similar hint</description>
      <id>443</id>
      <posX>57</posX>
      <posY>527</posY>
      <label>34039</label>
      <font>font12</font>
      <align>left</align>
      <textcolor>ff6FAFE1</textcolor>
      <visible>control.hastext(444)</visible>
    </control>
    <control>
      <type>textbox</type>
      <description>Similar artists box</description>
      <id>444</id>
      <posX>57</posX>
      <posY>556</posY>
      <width>400</width>
      <height>90</height>
      <label>#Play.Current.Lastfm.SimilarArtists</label>
      <font>font11</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>label</type>
      <description>tag hint</description>
      <id>332</id>
      <posX>527</posX>
      <posY>527</posY>
      <label>34041</label>
      <font>font12</font>
      <align>left</align>
      <textcolor>ff6FAFE1</textcolor>
      <visible>control.hastext(333)</visible>
    </control>
    <control>
      <type>textbox</type>
      <description>Track tags box</description>
      <id>333</id>
      <posX>527</posX>
      <posY>556</posY>
      <width>379</width>
      <height>90</height>
      <label>#Play.Current.Lastfm.TrackTags</label>
      <font>font11</font>
      <align>left</align>
      <textcolor>ivory</textcolor>
    </control>
    <control>
      <type>listcontrol</type>
      <description>current playlist control</description>
      <id>123</id>
      <posX>277</posX>
      <posY>180</posY>
      <height>444</height>
      <width>375</width>
      <onleft>10</onleft>
      <onright>123</onright>
      <font>font12</font>
      <font2>font12</font2>
      <font3>font12</font3>
      <textcolor2>ffffffff</textcolor2>
      <textXOff>3</textXOff>
      <textXOff2>56</textXOff2>
      <textXOff3>166</textXOff3>
      <textYOff>14</textYOff>
      <textYOff2>22</textYOff2>
      <textYOff3>22</textYOff3>
      <IconXOff>4</IconXOff>
      <IconYOff>7</IconYOff>
      <textureFocus>list_sub_focus.png</textureFocus>
      <textureNoFocus>list_sub_nofocus.png</textureNoFocus>
      <keepaspectratio>yes</keepaspectratio>
      <textureHeight>50</textureHeight>
      <itemHeight>50</itemHeight>
      <itemWidth>37</itemWidth>
      <spaceBetweenItems>2</spaceBetweenItems>
      <visible>player.hasmedia</visible>
    </control>
  </controls>
</window>
