<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>2</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <texture>BH Pictures.jpg</texture>
      <posX>-120</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>720</height>
      <colordiffuse>90ffffff</colordiffuse>
    </control>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <control>
      <description>General Overlay</description>
      <id>0</id>
      <type>image</type>
      <posX>0</posX>
      <posY>30</posY>
      <width>960</width>
      <height>720</height>
      <texture>general_overlay.png</texture>
    </control>
    <control>
      <type>actiongroup</type>
      <description>action menu</description>
      <defaultcontrol>2</defaultcontrol>
      <onexit>50</onexit>
      <buttonX>0</buttonX>
      <buttonY>-60</buttonY>
      <buttonwidth>79</buttonwidth>
      <buttonheight>720</buttonheight>
      <textureFocus>-</textureFocus>
      <textureNoFocus>Action_nofocus.png</textureNoFocus>
      <width>426</width>
      <height>900</height>
      <dimColor>00ffffff</dimColor>
      <control>
        <type>image</type>
        <id>0</id>
        <width>960</width>
        <height>720</height>
        <texture>white.png</texture>
        <colordiffuse>201e90ff</colordiffuse>
      </control>
      <control>
        <type>image</type>
        <posX>19</posX>
        <posY>78</posY>
        <width>280</width>
        <height>591</height>
        <texture>Backslide.png</texture>
        <colorDiffuse>fffffffff</colorDiffuse>
        <visible>!Control.HasFocus(50)</visible>
        <animation effect="slide" time="70" start="-300,0" reversible="false">visible</animation>
        <animation effect="slide" time="70" end="-300,0" reversible="false">hidden</animation>
      </control>
      <control>
        <description>group element</description>
        <type>group</type>
        <animation effect="slide" time="70" start="-300,0" reversible="false">visible</animation>
        <animation effect="slide" time="70" end="-300,0" reversible="false">hidden</animation>
        <posX>37</posX>
        <posY>121</posY>
        <layout>StackLayout</layout>
        <visible>!Control.HasFocus(50)</visible>
        <control>
          <description>View-As</description>
          <type>menubutton</type>
          <id>2</id>
          <label />
          <onright>50</onright>
          <onup>17</onup>
          <ondown>3</ondown>
          <mode>dialoglist</mode>
          <dialogTitle>792</dialogTitle>
          <valueTextInButton>yes</valueTextInButton>
          <valuePrefixText>95</valuePrefixText>
        </control>
        <control>
          <description>Sort</description>
          <type>sortbutton</type>
          <id>3</id>
          <label>103</label>
          <onright>50</onright>
          <ondown>5</ondown>
          <onup>2</onup>
        </control>
        <control>
          <description>Switch View</description>
          <type>menubutton</type>
          <id>5</id>
          <label />
          <onright>50</onright>
          <ondown>6</ondown>
          <onup>3</onup>
          <mode>dialoglist</mode>
          <dialogTitle>499</dialogTitle>
          <valueTextInButton>yes</valueTextInButton>
          <valuePrefixText>97</valuePrefixText>
        </control>
        <control>
          <description>SlideShow button</description>
          <type>button</type>
          <id>6</id>
          <label>108</label>
          <onright>50</onright>
          <ondown>7</ondown>
          <onup>5</onup>
        </control>
        <control>
          <description>Recursive Slideshow</description>
          <type>button</type>
          <id>7</id>
          <label>361</label>
          <onright>50</onright>
          <ondown>9876</ondown>
          <onup>6</onup>
        </control>
        <control>
          <description>Picture Settings</description>
          <type>button</type>
          <id>9876</id>
          <label>5</label>
          <hyperlink>12</hyperlink>
          <onup>7</onup>
          <ondown>2</ondown>
          <onright>50</onright>
        </control>
      </control>
    </control>
    <import>common.facade.pictures.xml</import>
    <control>
      <id>500</id>
      <type>image</type>
      <posX>742</posX>
      <posY>180</posY>
      <width>195</width>
      <height>180</height>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <texture mask="Picturethumbmask.png">#selectedthumb</texture>
      <visible>[facadeview.list|facadeview.album]+[Control.HasFocus(50)]</visible>
      <animation effect="fade" time="70" reversible="false">visible</animation>
    </control>
    <control>
      <type>image</type>
      <id>1</id>
      <posX>742</posX>
      <posY>180</posY>
      <width>195</width>
      <height>180</height>
      <texture mask="Picturethumbmask.png">Picturethumbframe.png</texture>
      <visible>[facadeview.list|facadeview.album]+[Control.HasFocus(50)]</visible>
      <animation effect="fade" time="70" reversible="false">visible</animation>
    </control>
  </controls>
</window>
