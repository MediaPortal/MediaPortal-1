	  <animation effect="fade" time="250">WindowOpen</animation>
	  <animation effect="fade" time="250">WindowClose</animation>
		<visible allowhiddenfocus="true">[control.HasFocus(200) | control.HasFocus(300) | control.HasFocus(400) | control.HasFocus(500) | control.HasFocus(600) | control.HasFocus(700)]</visible> 
  </control>
</controls>
