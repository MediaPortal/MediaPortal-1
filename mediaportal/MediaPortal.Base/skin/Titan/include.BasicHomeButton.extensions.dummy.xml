<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls> 
  <control>
	 <description>BasicHome DUMMY Focus</description>
	 <type>image</type>
	 <id>0</id>
   <posX>1264</posX>
   <posY>607</posY>
   <width>624</width>
   <height>271</height>
	 <texture>basichome_button_dummy.png</texture>
	 <animation effect="fade" time="250" condition="!control.isvisible(2000)">conditional</animation>
 	 <animation effect="slide" start="700" end="0" time="600">visible</animation>
	 <animation effect="fade" time="250">visible</animation>
	 <animation effect="zoom" start="100,100" end="102,102" time="200" condition="control.hasfocus(300)">conditional</animation>
