	 <animation effect="fade" time="250">WindowClose</animation>
	 <visible>control.HasFocus(200) | control.HasFocus(300) | control.HasFocus(400) | control.HasFocus(500) | control.HasFocus(600) | control.HasFocus(700)</visible>     
	</control>     
  <control>
    <description>BasicHome DUMMY</description>
    <type>button</type>
    <id>300</id>
		<posX>1274</posX>
	  <posY>613</posY>
    <width>594</width>
    <height>241</height> 
    <textureFocus>-</textureFocus>
    <textureNoFocus>-</textureNoFocus>
    <label></label>
    <font>TitanLight20</font>
    <textcolor>FFFFFF</textcolor>
		<textalign>center</textalign>   
    <textcolorNoFocus>FFFFFFFF</textcolorNoFocus>
    <textYOff>176</textYOff>
		<onright>600</onright>
		<onleft>700</onleft>
		<onup>200</onup>
		<ondown>12</ondown>
