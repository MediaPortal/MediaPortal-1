        <height>47</height>
        <texture>logos\#Play.Current.VideoCodec.Texture.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Video resolution</description>
        <type>image</type>
        <id>0</id>
        <width>69</width>
        <height>47</height>
        <texture>logos\#Play.Current.VideoResolution.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
        <description>Aspect ratio</description>
        <type>image</type>
        <id>0</id>
        <width>69</width>
        <height>47</height>
        <texture>logos\#Play.Current.AspectRatio.png</texture>
        <keepaspectratio>yes</keepaspectratio>
      </control>
      <control>
