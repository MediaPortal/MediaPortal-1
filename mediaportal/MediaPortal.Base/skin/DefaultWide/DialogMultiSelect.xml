<window>
	<id>2100</id>
	<defaultcontrol>3</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control>
			<type>image</type>
			<id>0</id>
			<width>1280</width>
			<height>720</height>
			<texture>white.png</texture>
      <colordiffuse>201e90ff</colordiffuse>
		</control>
		<control>
			<type>image</type>
			<id>0</id>
			<posX>206</posX>
			<posY>80</posY>
			<width>970</width>
			<height>530</height>
			<texture>context_background.png</texture>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="250">WindowClose</animation>
		</control>
		<control>
			<description>GFX-Heading "menu" text label</description>
			<type>label</type>
			<id>5</id>
			<posX>291</posX>
			<posY>110</posY>
			<width>426</width>
			<label>924</label>
			<font>font14</font>
			<textcolor>Black</textcolor>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="250">WindowClose</animation>
			<visible>false</visible>
		</control>
		<control>
			<description>Heading text label</description>
			<type>label</type>
			<id>4</id>
			<posX>291</posX>
			<posY>110</posY>
			<label>181</label>
			<font>font14</font>
			<textcolor>FFFFFFFF</textcolor>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="250">WindowClose</animation>
		</control>
		<control>
			<description>Exit button</description>
			<type>button</type>
			<id>2</id>
			<posX>1055</posX>
			<posY>105</posY>
			<width>69</width>
			<height>59</height>
			<textureFocus>top_exit_enabled.png</textureFocus>
			<textureNoFocus>top_exit_disabled.png</textureNoFocus>
			<label>-</label>
			<onleft>3</onleft>
			<onright>3</onright>
			<onup>3</onup>
			<ondown>3</ondown>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="250">WindowClose</animation>
		</control>
		<control>
			<description>options listcontrol</description>
			<type>listcontrol</type>
			<id>3</id>
			<posX>284</posX>
			<posY>187</posY>
			<width>800</width>
			<height>325</height>
			<onup>2</onup>
			<onleft>10</onleft>
			<onright>10</onright>
			<spinPosX>977</spinPosX>
			<spinPosY>472</spinPosY>
			<textXOff>88</textXOff>
			<PinIconXOff>60</PinIconXOff>
			<PinIconYOff>10</PinIconYOff>
			<PinIconWidth>40</PinIconWidth>
			<PinIconHeight>25</PinIconHeight>
			<keepaspectratio>yes</keepaspectratio>
      <scrollOffset>3</scrollOffset>
			<spinCanFocus>no</spinCanFocus>
			<itemHeight>34</itemHeight>
			<itemWidth>53</itemWidth>
			<IconXOff>17</IconXOff>
			<IconYOff>3</IconYOff>
			<animation effect="fade" time="250">WindowOpen</animation>
			<animation effect="fade" time="250">WindowClose</animation>
		</control>
		<control>
			<description>Okay button</description>
			<type>button</type>
			<id>10</id>
			<label>186</label>
			<textalign>center</textalign>
			<textYOff>0</textYOff>
			<posX>400</posX>
			<posY>515</posY>
			<onleft>3</onleft>
			<onright>11</onright>
			<onup>3</onup>
			<ondown>3</ondown>
			<textureFocus>tvguide_button_middle.png</textureFocus>
			<textureNoFocus>tvguide_button_light_middle.png</textureNoFocus>
		</control>
		<control>
			<description>Cancel button</description>
			<type>button</type>
			<id>11</id>
			<label>510</label>
			<textalign>center</textalign>
			<textYOff>0</textYOff>
			<posX>740</posX>
			<posY>515</posY>
			<onleft>10</onleft>
			<onright>3</onright>
			<onup>3</onup>
			<ondown>3</ondown>
