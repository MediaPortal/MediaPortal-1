<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>2005</id>
  <defaultcontrol>13</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <disabletopbar>false</disabletopbar>
  <controls>
    <control>
      <description>background image (not used!)</description>
      <type>image</type>
      <id>105</id>
      <posX>2180</posX>
      <posY>628</posY>
      <width>1000</width>
      <height>89</height>
      <texture>white.png</texture>
      <colordiffuse>dd1a2e55</colordiffuse>
      <visible>window.ispauseosdvisible|player.rewinding|player.forwarding</visible>
    </control>
    <control>
      <description>bar image</description>
      <type>image</type>
      <id>111</id>
      <posX>81</posX>
      <posY>566</posY>
      <height>129</height>
      <width>1124</width>
      <texture>osd_mask3.png</texture>
      <colordiffuse>ddffffff</colordiffuse>
      <visible>!window.isosdvisible+[window.ispauseosdvisible+player.paused|player.rewinding|player.forwarding]</visible>
    </control>
    <control>
      <description>movie title</description>
      <type>label</type>
      <id>120</id>
      <posX>109</posX>
      <posY>581</posY>
      <width>739</width>
      <label>#title</label>
      <align>left</align>
      <font>font16</font>
      <textcolor>ivory</textcolor>
      <visible>control.isVisible(111)</visible>
    </control>
    <control>
      <description>progress bar background</description>
      <type>image</type>
      <id>104</id>
      <posX>219</posX>
      <posY>646</posY>
      <width>689</width>
      <height>19</height>
      <texture>osd_progress_background.png</texture>
      <visible>control.isVisible(111)</visible>
    </control>
    <control>
      <description>Progress Bar</description>
      <type>progress</type>
      <id>1</id>
      <posX>220</posX>
      <posY>647</posY>
      <width>686</width>
      <offset>0</offset>
      <height>17</height>
      <innerheight>17</innerheight>
      <lefttexture>osd_progress_left.png</lefttexture>
      <midtexture>osd_progress_mid.png</midtexture>
      <righttexture>osd_progress_right.png</righttexture>
      <label>#percentage</label>
      <visible>control.isVisible(111)</visible>
    </control>
    <control>
      <description>Start Time Info</description>
      <type>label</type>
      <id>101</id>
      <posX>109</posX>
      <posY>640</posY>
      <label>#currentplaytime</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>ffffffff</textcolor>
      <visible>control.isVisible(111)</visible>
    </control>
    <control>
      <description>Remaining time</description>
      <type>label</type>
      <id>102</id>
      <posX>928</posX>
      <posY>640</posY>
      <label>#currentremaining</label>
      <align>left</align>
      <font>font14</font>
      <textcolor>ffffffff</textcolor>
      <visible>control.isVisible(111)</visible>
    </control>
    <control>
      <description>Time BG</description>
      <type>image</type>
      <id>4321</id>
      <posX>1050</posX>
      <posY>571</posY>
      <width>145</width>
      <height>35</height>
      <texture>button_focus.png</texture>
      <colordiffuse>90ffffff</colordiffuse>
      <visible>control.isVisible(111)</visible>
    </control>
    <control>
      <description>Time Info</description>
      <type>label</type>
      <id>140</id>
      <posX>1179</posX>
      <posY>572</posY>
      <label>#time</label>
      <align>right</align>
      <font>font16</font>
      <textcolor>ivory</textcolor>
      <visible>control.isVisible(111)</visible>
    </control>
    <import>common.play.xml</import>
  </controls>
</window>
