<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>1</id>
  <defaultcontrol>8</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#header.label:</define>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <texture>background.png</texture>
      <width>1280</width>
      <height>720</height>
    </control>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <control>
      <description>General Overlay</description>
      <id>0</id>
      <type>image</type>
      <posX>0</posX>
      <posY>30</posY>
      <width>1280</width>
      <height>765</height>
      <texture>general_overlay.png</texture>
    </control>
    <!--control>
    <description>Overlay buttons left</description>
    <id>0</id>
    <type>image</type>
    <posX>50</posX>
    <posY>156</posY>
    <width>300</width>
    <height>265</height>
    <texture mask="picturethumbmask.png">black.png</texture>
    <colordiffuse>60ffffff</colordiffuse>
  </control-->
  <control>
    <type>group</type>
    <layout>StackLayout(0,Vertical,true)</layout>
    <description>group element</description>
    <posX>70</posX>
    <posY>170</posY>
    <control>
      <description>TV On/Off button</description>
      <type>togglebutton</type>
      <id>8</id>
      <label>707</label>
      <onup>17</onup>
      <ondown>7</ondown>
      <onright>99</onright>
    </control>
    <control>
      <description>Channel button</description>
      <type>button</type>
      <id>7</id>
      <label>602</label>
      <onup>8</onup>
      <ondown>2</ondown>
      <onright>99</onright>
    </control>
    <control>
      <description>TVGuide</description>
      <type>button</type>
      <id>2</id>
      <label>600</label>
      <hyperlink>600</hyperlink>
      <onup>7</onup>
      <ondown>11</ondown>
      <onright>99</onright>
    </control>
    <control>
      <description>Recordings</description>
      <type>button</type>
      <id>11</id>
      <label>604</label>
      <hyperlink>603</hyperlink>
      <onup>2</onup>
      <ondown>3</ondown>
      <onright>99</onright>
    </control>
    <control>
      <description>Record Now</description>
      <type>button</type>
      <id>3</id>
      <label>601</label>
      <onup>11</onup>
      <ondown>14</ondown>
      <onright>99</onright>
    </control>
    <control>
      <description>Active Recordings</description>
      <type>button</type>
      <id>14</id>
      <label>200052</label>
      <onup>3</onup>
      <ondown>9</ondown>
      <onright>99</onright>
    </control>
    <control>
      <description>Active Streams</description>
      <type>button</type>
      <id>9</id>
      <label>692</label>
      <onup>14</onup>
      <ondown>10</ondown>
      <onleft>99</onleft>
    </control>
    <control>
      <description>Scheduler</description>
      <type>button</type>
      <id>10</id>
      <label>603</label>
      <hyperlink>601</hyperlink>
      <onup>9</onup>
      <ondown>12</ondown>
      <onleft>99</onleft>
    </control>
    <control>
      <description>Search</description>
      <type>button</type>
      <id>12</id>
      <label>137</label>
      <hyperlink>604</hyperlink>
      <onup>10</onup>
      <ondown>13</ondown>
      <onleft>99</onleft>
    </control>
    <control>
      <description>Teletext</description>
      <type>button</type>
      <id>13</id>
      <label>1441</label>
      <onup>12</onup>
      <ondown>76</ondown>
      <onleft>99</onleft>
      <visible>no</visible>
    </control>
    <control>
      <description>TV Shows</description>
      <type>button</type>
      <id>76</id>
      <hyperlink>25</hyperlink>
      <label>3</label>
      <onup>13</onup>
      <ondown>77</ondown>
      <onleft>99</onleft>
      <visible>plugin.isenabled(MP-TV Series)</visible>
    </control>
    <control>
      <description>Videos</description>
      <type>button</type>
      <id>77</id>
      <hyperlink>9811</hyperlink>
      <label>1300</label>
      <onup>76</onup>
      <ondown>8</ondown>
      <onleft>99</onleft>
      <visible>plugin.isenabled(Videos)</visible>
    </control>
  </control>
  <control>
    <description>LCD image</description>
    <type>image</type>
    <id>0</id>
    <posX>500</posX>
    <posY>80</posY>
    <width>480</width>
    <height>279</height>
    <texture>LCD.png</texture>
  </control>
  <control>
    <description>video window</description>
    <type>videowindow</type>
    <id>99</id>
    <posX>510</posX>
    <posY>90</posY>
    <width>460</width>
    <height>259</height>
    <onleft>8</onleft>
    <onright>9</onright>
    <action>18</action>
    <textureFocus>tv_green_border.png</textureFocus>
  </control>
  <control>
    <description>background</description>
    <type>image</type>
    <id>0</id>
    <posX>610</posX>
    <posY>380</posY>
    <width>254</width>
    <height>19</height>
    <texture>osd_progress_background_medium.png</texture>
    <visible>yes</visible>
  </control>
  <control>
    <description>Progress Bar</description>
    <type>progress</type>
    <id>1</id>
    <posX>598</posX>
    <posY>381</posY>
    <width>278</width>
    <height>17</height>
    <label>#TV.View.Percentage</label>
    <texturebg>-</texturebg>
    <onlymidtexture>no</onlymidtexture>
    <midwidth>788</midwidth>
    <midheight>17</midheight>
    <midoffsetX>0</midoffsetX>
    <midoffsetY>0</midoffsetY>
    <midtexture>osd_progress_mid.png</midtexture>
    <visible>yes</visible>
  </control>
  <control>
    <description>Seperator</description>
    <type>image</type>
    <id>1</id>
    <posX>400</posX>
    <posY>425</posY>
    <width>820</width>
    <height>1</height>
    <texture>hbar1white.png</texture>
    <colordiffuse>60ffffff</colordiffuse>
  </control>
  <control>
    <description>Playing Channel</description>
    <type>image</type>
    <id>1</id>
    <posX>400</posX>
    <posY>440</posY>
    <width>60</width>
    <height>60</height>
    <align>right</align>
    <keepaspectratio>yes</keepaspectratio>
    <texture>#TV.View.thumb</texture>
  </control>
  <control>
    <description>Current program title</description>
    <type>fadelabel</type>
    <id>131</id>
    <posX>480</posX>
    <posY>440</posY>
    <width>720</width>
    <label>#TV.View.start - #TV.View.stop  #TV.View.title</label>
    <font>font13</font>
    <align>left</align>
    <textcolor>ff6FAFE1</textcolor>
  </control>
  <control>
    <description>Description1</description>
    <type>textboxscrollup</type>
    <id>15</id>
    <posX>480</posX>
    <posY>480</posY>
    <width>750</width>
    <height>155</height>
    <label>#TV.View.description</label>
    <font>font11</font>
    <textcolor>ivory</textcolor>
  </control>
  <control>
    <description>record pin</description>
    <type>image</type>
    <id>24</id>
    <posX>415</posX>
    <posY>610</posY>
    <texture>tvguide_record_button.png</texture>
    <visible>no</visible>
  </control>
</controls>
</window>
