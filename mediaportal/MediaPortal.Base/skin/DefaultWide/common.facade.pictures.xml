<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!Control.HasFocus(50)</visible>
      <animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
      <animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
          <unfocusedAlpha>160</unfocusedAlpha>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>60</posX>
          <posY>180</posY>
          <width>1220</width>
          <height>500</height>
          <scrollOffset>1</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <itemWidth>145</itemWidth>
          <itemHeight>105</itemHeight>
          <textureWidth>130</textureWidth>
          <textureHeight>90</textureHeight>
          <thumbWidth>130</thumbWidth>
          <thumbHeight>90</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <keepaspectratio>yes</keepaspectratio>
          <itemWidthBig>200</itemWidthBig>
          <itemHeightBig>140</itemHeightBig>
          <textureWidthBig>190</textureWidthBig>
          <textureHeightBig>135</textureHeightBig>
          <thumbWidthBig>190</thumbWidthBig>
          <thumbHeightBig>135</thumbHeightBig>
          <thumbPosXBig>0</thumbPosXBig>
          <thumbPosYBig>0</thumbPosYBig>
          <zoomXPixels>50</zoomXPixels>
          <zoomYPixels>50</zoomYPixels>
          <hideUnfocusTexture>yes</hideUnfocusTexture>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>no</renderUnfocusText>
          <showFrame>true</showFrame>
          <textureMask>Picturethumbmask.png</textureMask>
          <frameNoFocus>Picturethumbframe.png</frameNoFocus>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <imageFolder>-</imageFolder>
          <imageFolderFocus>-</imageFolderFocus>
          <keepaspectratio>yes</keepaspectratio>
          <font>font10</font>
          <spinCanFocus>no</spinCanFocus>
          <dimColor>ffffffff</dimColor>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <colordiffuse>90ffffff</colordiffuse>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <posX>60</posX>
          <posY>280</posY>
          <width>1200</width>
          <height>720</height>
          <onup>2</onup>
          <onleft>2</onleft>
          <itemWidth>190</itemWidth>
          <itemHeight>168</itemHeight>
          <textureWidth>190</textureWidth>
          <textureHeight>130</textureHeight>
          <thumbWidth>192</thumbWidth>
          <thumbHeight>130</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <imageFolder>Picturethumbframe.png</imageFolder>
          <imageFolderFocus>Picturethumbframe.png</imageFolderFocus>
          <frame>Picturethumbframe.png</frame>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <textureMask>Picturethumbmask.png</textureMask>
          <thumbs mask="Picturethumbmask.png"></thumbs>
          <background>emtpy.png</background>
          <InfoImage></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <keepaspectratio>no</keepaspectratio>
          <dimColor>80ffffff</dimColor>
          <spinPosX>-800</spinPosX>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>220</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <thumbAnimation effect="zoom" start="100,100" end="180,180" time="200" tween="circle" ease="inout">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="180,180" end="100,100" time="200" tween="back" ease="inout">unfocus</thumbAnimation>
        </control>
        <control>
          <description>Cover Flow view</description>
          <type>coverflow</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>0</posX>
          <posY>190</posY>
          <width>1366</width>
          <height>768</height>
          <onleft>2</onleft>
          <onright>2</onright>
          <onup>2</onup>
          <ondown>2</ondown>
          <selectedCard>0</selectedCard>
          <cardWidth>450</cardWidth>
          <cardHeight>300</cardHeight>
          <angle>65</angle>
          <sideShift>500</sideShift>
          <sideGap>100</sideGap>
          <sideDepth>700</sideDepth>
          <offsetY>0</offsetY>
          <selectedOffsetY>20</selectedOffsetY>
          <speed>8</speed>
          <backgroundColor>000000FF</backgroundColor>
          <backgroundHeight>100</backgroundHeight>
          <backgroundWidth>1366</backgroundWidth>
          <backgroundX>0</backgroundX>
          <backgroundY>355</backgroundY>
          <backgroundDiffuse>60000000</backgroundDiffuse>
          <background>roof.png</background>
          <showBackground>yes</showBackground>
          <showFrame>yes</showFrame>
          <frame>picturethumbframe.png</frame>
          <frameFocus>picturethumbframe.png</frameFocus>
          <frameWidth>450</frameWidth>
          <frameHeight>300</frameHeight>
          <spinSpeed>8</spinSpeed>
          <unfocusedAlpha>FF</unfocusedAlpha>
          <folderPrefix></folderPrefix>
          <folderSuffix></folderSuffix>
          <font1>font13</font1>
          <font2>font10</font2>
          <label1></label1>
          <label2></label2>
          <keepaspectratio>no</keepaspectratio>
          <textColor>FFFFFFFF</textColor>
          <remoteColor>FFFF0000</remoteColor>
          <playedColor>FFA0D0FF</playedColor>
          <downloadColor>FF00FF00</downloadColor>
          <selectedColor>FFFFFFFF</selectedColor>
          <shadowDistance>1</shadowDistance>
          <label1YOff>380</label1YOff>
          <label2YOff>390</label2YOff>
          <pageSize>100</pageSize>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>370</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <cards mask="picturethumbmask.png">black.png</cards>
          <subitems>
            <subitem>
              <![CDATA[                                <controls>
              <control>
                <description>background</description>
                <type>image</type>
                <posX>0</posX>
                <posY>0</posY>
                <width>450</width>
                <height>300</height>
                <texture mask="picturethumbmask.png">blue.png</texture>
              </control>
              <!--control>
              <description>Info Header</description>
              <type>label</type>
              <posX>10</posX>
              <posY>30</posY>
              <width>400</width>
              <label>Details</label>
              <font>Label</font>
              <align>center</align>
              <textcolor>ivory</textcolor>
              <shadowAngle>0</shadowAngle>
              <shadowDistance>0</shadowDistance>
            </control-->
            <control>
              <description>General Info</description>
              <type>label</type>
              <posX>10</posX>
              <posY>60</posY>
              <width>400</width>
              <label>#selecteditem</label>
              <font>font12</font>
              <align>center</align>
              <textcolor>ivory</textcolor>
              <shadowAngle>0</shadowAngle>
              <shadowDistance>0</shadowDistance>
            </control>
            <control>
              <description>Picture size</description>
              <type>label</type>
              <posX>10</posX>
              <posY>90</posY>
              <width>400</width>
              <label>#selecteditem2</label>
              <font>font12</font>
              <align>center</align>
              <textcolor>ivory</textcolor>
              <shadowAngle>0</shadowAngle>
              <shadowDistance>0</shadowDistance>
            </control>
          </controls>]]></subitem>
      </subitems>
    </control>
  </control>
</control>
</controls>
</window>
