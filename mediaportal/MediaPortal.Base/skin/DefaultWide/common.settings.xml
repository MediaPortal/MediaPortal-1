<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <texture>settings.jpg</texture>
      <width>1280</width>
      <height>720</height>
      <colordiffuse>ffffffff</colordiffuse>
    </control>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <texture>background.png</texture>
      <width>1280</width>
      <height>720</height>
      <visible>skin.hassetting(#skin.no.category.fanart)</visible>
    </control>
    <control>
      <description>General Overlay</description>
      <id>0</id>
      <type>image</type>
      <posX>0</posX>
      <posY>-70</posY>
      <width>1280</width>
      <height>970</height>
      <texture>general_overlay.png</texture>
    </control>
    <control>
      <description>background top overlay</description>
      <type>image</type>
      <id>1</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>270</height>
      <texture>overlay_top.png</texture>
      <colordiffuse>ccffffff</colordiffuse>
    </control>
    <control>
      <type>label</type>
      <id>1</id>
      <posX>30</posX>
      <posY>10</posY>
      <label>#(iif(and(neq(L(102012), #currentmodule), neq(L(102002), #currentmodule), neq(L(100100), #currentmodule), neq(L(102015), #currentmodule)), #currentmodule, ' '))</label>
      <font>font11</font>
      <align>left</align>
      <textcolor>ff6FAFE1</textcolor>
    </control>
  </controls>
</window>
