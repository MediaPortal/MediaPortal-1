<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
      <animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <height>300</height>
          <playedColor>ffffffff</playedColor>
          <playedColor2>ffffffff</playedColor2>
          <textcolor>ff99f9ff</textcolor>
          <textcolor2>ff99f9ff</textcolor2>
          <textcolorNoFocus>ff99f9ff</textcolorNoFocus>
        </control>
        <control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
          <posX>100</posX>
          <posY>170</posY>
          <height>300</height>
          <width>800</width>
          <textXOff>10</textXOff>
          <textYOff>2</textYOff>
          <textXOff2>800</textXOff2>
          <textYOff2>32</textYOff2>
          <textXOff3>76</textXOff3>
          <textYOff3>32</textYOff3>
          <PinIconXOff>80</PinIconXOff>
          <PinIconYOff>34</PinIconYOff>
          <PinIconWidth>35</PinIconWidth>
          <PinIconHeight>25</PinIconHeight>
          <textureHeight>60</textureHeight>
          <itemWidth>55</itemWidth>
          <itemHeight>55</itemHeight>
          <IconXOff>10</IconXOff>
          <IconYOff>2</IconYOff>
          <keepaspectratio>yes</keepaspectratio>
          <font>font13</font>
          <font2>font10</font2>
          <font3>font10</font3>
          <playedColor>ffffffff</playedColor>
          <playedColor2>ffffffff</playedColor2>
          <textcolor>ff99f9ff</textcolor>
          <textcolor2>ff99f9ff</textcolor2>
          <textcolor3>ff99f9ff</textcolor3>
          <textcolorNoFocus>ff99f9ff</textcolorNoFocus>
          <spinWidth>40</spinWidth>
          <spinHeight>40</spinHeight>
          <spinPosX>2170</spinPosX>
          <spinPosY>470</spinPosY>
          <spaceBetweenItems>0</spaceBetweenItems>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>2</ondown>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>100</posX>
          <posY>170</posY>
          <width>1200</width>
          <height>300</height>
          <scrollOffset>1</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <itemWidth>140</itemWidth>
          <itemHeight>85</itemHeight>
          <textureWidth>124</textureWidth>
          <textureHeight>70</textureHeight>
          <thumbWidth>124</thumbWidth>
          <thumbHeight>70</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <itemWidthBig>225</itemWidthBig>
          <itemHeightBig>130</itemHeightBig>
          <thumbWidthBig>200</thumbWidthBig>
          <thumbHeightBig>113</thumbHeightBig>
          <textureWidthBig>200</textureWidthBig>
          <textureHeightBig>113</textureHeightBig>
          <thumbPosXBig>0</thumbPosXBig>
          <thumbPosYBig>0</thumbPosYBig>
          <zoomXPixels>30</zoomXPixels>
          <zoomYPixels>30</zoomYPixels>
          <hideUnfocusTexture>yes</hideUnfocusTexture>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>no</renderUnfocusText>
          <showFrame>true</showFrame>
          <textureMask>Picturethumbmask.png</textureMask>
          <frameNoFocus>Picturethumbframe.png</frameNoFocus>
          <frameFocus>Picturethumbframe.png</frameFocus>
          <imageFolder>-</imageFolder>
          <imageFolderFocus>-</imageFolderFocus>
          <keepaspectratio>no</keepaspectratio>
          <font>font10</font>
          <spinPosX>2170</spinPosX>
          <spinPosY>470</spinPosY>
          <dimColor>ffffffff</dimColor>
        </control>
      </control>
    </control>
  </controls>
</window>
