<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <id>2</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>1</id>
      <texture>BH Pictures.jpg</texture>
      <width>1920</width>
      <height>1080</height>
      <colordiffuse>90ffffff</colordiffuse>
    </control>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <control>
      <description>General Overlay</description>
      <id>0</id>
      <type>image</type>
      <posX>0</posX>
      <posY>45</posY>
      <width>1920</width>
      <height>1080</height>
      <texture>general_overlay.png</texture>
    </control>
    <control>
      <type>actiongroup</type>
      <description>action menu</description>
      <defaultcontrol>2</defaultcontrol>
      <onexit>50</onexit>
      <buttonX>0</buttonX>
      <buttonY>-60</buttonY>
      <buttonwidth>159</buttonwidth>
      <buttonheight>1080</buttonheight>
      <textureFocus>-</textureFocus>
      <textureNoFocus>Action_nofocus.png</textureNoFocus>
      <width>852</width>
      <height>1350</height>
      <dimColor>00ffffff</dimColor>
      <control>
        <type>image</type>
        <id>0</id>
        <width>1920</width>
        <height>1080</height>
        <texture>white.png</texture>
        <colordiffuse>50005eef</colordiffuse>
      </control>
      <control>
        <type>image</type>
        <posX>39</posX>
        <posY>117</posY>
        <width>480</width>
        <height>886</height>
        <texture>Backslide.png</texture>
        <colorDiffuse>fffffffff</colorDiffuse>
        <visible>!Control.HasFocus(50)</visible>
        <animation effect="slide" time="70" start="-300,0" reversible="false">visible</animation>
        <animation effect="slide" time="70" end="-300,0" reversible="false">hidden</animation>
      </control>
      <control>
        <description>group element</description>
        <type>group</type>
        <animation effect="slide" time="70" start="-300,0" reversible="false">visible</animation>
        <animation effect="slide" time="70" end="-300,0" reversible="false">hidden</animation>
        <posX>75</posX>
        <posY>181</posY>
        <layout>StackLayout</layout>
        <visible>!Control.HasFocus(50)</visible>
        <control>
          <description>View-As</description>
          <type>menubutton</type>
          <id>2</id>
          <label />
          <onright>50</onright>
          <onup>17</onup>
          <ondown>3</ondown>
          <mode>dialoglist</mode>
          <dialogTitle>792</dialogTitle>
          <valueTextInButton>yes</valueTextInButton>
          <valuePrefixText>95</valuePrefixText>
        </control>
        <control>
          <description>Sort</description>
          <type>sortbutton</type>
          <id>3</id>
          <label>103</label>
          <onright>50</onright>
          <ondown>5</ondown>
          <onup>2</onup>
        </control>
        <control>
          <description>Switch View</description>
          <type>menubutton</type>
          <id>5</id>
          <label />
          <onright>50</onright>
          <ondown>6</ondown>
          <onup>3</onup>
          <mode>dialoglist</mode>
          <dialogTitle>499</dialogTitle>
          <valueTextInButton>yes</valueTextInButton>
          <valuePrefixText>97</valuePrefixText>
        </control>
        <control>
          <description>SlideShow button</description>
          <type>button</type>
          <id>6</id>
          <label>108</label>
          <onright>50</onright>
          <ondown>7</ondown>
          <onup>5</onup>
        </control>
        <control>
          <description>Recursive Slideshow</description>
          <type>button</type>
          <id>7</id>
          <label>361</label>
          <onright>50</onright>
          <ondown>9876</ondown>
          <onup>6</onup>
        </control>
        <control>
          <description>Slideshow Settings</description>
          <type>button</type>
          <id>9</id>
          <label>300187</label>
          <onright>50</onright>
          <onleft>50</onleft>
          <onup>7</onup>
          <ondown>8</ondown>
          <hyperlink>12</hyperlink>
        </control>
        <control>
          <description>Search</description>
          <type>button</type>
          <id>8</id>
          <label>137</label>
          <onright>50</onright>
          <onleft>50</onleft>
          <onup>9</onup>
          <ondown>9876</ondown>
        </control>
        <control>
          <description>Picture Settings</description>
          <type>button</type>
          <id>9876</id>
          <label>5</label>
          <hyperlink>12</hyperlink>
          <onup>7</onup>
          <ondown>10</ondown>
          <onright>50</onright>
        </control>
        <control>
          <description>MP PhotoSlideShow</description>
          <type>button</type>
          <id>10</id>
          <label>MP PhotoSlideshow</label>
          <hyperlink>58963</hyperlink>
          <onup>9876</onup>
          <ondown>2</ondown>
          <onright>50</onright>
          <visible>plugin.isenabled(MPPhotoSlideshow)</visible>
        </control>
      </control>
    </control>
    <import>common.facade.pictures.xml</import>
    <control>
      <id>500</id>
      <type>image</type>
      <posX>1410</posX>
      <posY>300</posY>
      <width>450</width>
      <height>550</height>
      <keepaspectratio>yes</keepaspectratio>
      <centered>yes</centered>
      <texture>#(iif(eq(#selecteditem,'..'),'','#selectedthumb'))</texture>
      <visible>facadeview.playlist|facadeview.list+Control.HasFocus(50)</visible>
    </control>
  </controls>
</window>
