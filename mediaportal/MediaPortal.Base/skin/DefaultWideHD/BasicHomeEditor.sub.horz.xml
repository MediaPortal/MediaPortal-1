        <animation effect="fade" start="100" end="0" time="250" reversible="false" condition="string.contains(#hedit.sub.calc.nav,b1)">conditional</animation>
        <animation effect="fade" start="100" end="0" time="0" condition="!string.contains(#hedit.sub.calc.nav,b)">conditional</animation>
