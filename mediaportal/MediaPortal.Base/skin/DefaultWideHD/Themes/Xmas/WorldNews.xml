          <description>News</description>
          <type>button</type>
          <id>2</id>
          <label>News...</label>
          <onup>9</onup>
          <ondown>3</ondown>
        </control>
        <control>
          <description>Merge news</description>
          <type>checkbutton</type>
          <id>3</id>
          <label>Merge news</label>
          <onup>2</onup>
          <ondown>4</ondown>
        </control>
        <control>
          <description>View</description>
