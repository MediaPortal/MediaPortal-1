  <Listing type="Html">
    <Site url="http://www.tvmovie.de/tv/sender-[ID]?date=[YYYY]-[MM]-[DD]&amp;type=day" external="false" encoding="utf-8" post="" />
    <Html>
      <Template name="default" start="&lt;section class=&quot;epgList epgList--channel&quot;&gt;" end="&lt;/section&gt;">
        <SectionTemplate tags="DH">
          <TemplateText>
            <![CDATA[<div class="item clear table">
    <div class="td first big">
        <div></div>
    </div>
    <div class="time-wrapper"><#START></div>
    <div class="epg-col-broadcast-name">
        <h3 class="bold"><#TITLE></h3>
        <div class="small-font"><#GENRE>)<#SUBTITLE></div>
    </div>
</div>]]></TemplateText>
        </SectionTemplate>
      </Template>
      <Template name="Details" start="&lt;div class=&quot;text&quot;&gt;" end="&lt;section">
