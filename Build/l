!insertmacro: RunUninstaller
ReadRegStr $R0 HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\MediaPortal TV Server\UninstallString
!insertmacro: _If
!insertmacro: end of _If
HideWindow
!insertmacro: GetParentCall
!insertmacro: end of GetParentCall
ClearErrors
CopyFiles: "$R0" -> "$TEMP\uninstall-temp.exe", size=0KB
!insertmacro: _If
!insertmacro: end of _If
!insertmacro: _And
!insertmacro: end of _And
ExecWait: ""$TEMP\uninstall-temp.exe" _?=$R1" (->)
!insertmacro: _Else
!insertmacro: end of _Else
ExecWait: ""$TEMP\uninstall-temp.exe" /frominstall _?=$R1" (->)
!insertmacro: _EndIf
!insertmacro: end of _EndIf
!insertmacro: _EndIf
!insertmacro: end of _EndIf
!insertmacro: end of RunUninstaller
!insertmacro: _Else
!insertmacro: end of _Else
!insertmacro: RunUninstaller
ReadRegStr $R0 HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\MediaPortal TV Server\UninstallString
!insertmacro: _If
!insertmacro: end of _If
!insertmacro: GetParentCall
!insertmacro: end of GetParentCall
ClearErrors
CopyFiles: "$R0" -> "$TEMP\uninstall-temp.exe", size=0KB
!insertmacro: _If
!insertmacro: end of _If
!insertmacro: _And
!insertmacro: end of _And
