!insertmacro: UnselectSection
Push: $0
Push: $1
StrCpy $1 "2" () ()
SectionGetFlags: $1->$0
IntOp: $0=$0&0xFFFFFFFE
SectionSetFlags: $1->$0
Pop: $1
Pop: $0
!insertmacro: end of UnselectSection
!insertmacro: SetSectionFlag
Push: $R0
Push: $R1
StrCpy $R1 "2" () ()
SectionGetFlags: $R1->$R0
IntOp: $R0=$R0|16
SectionSetFlags: $R1->$R0
Pop: $R1
Pop: $R0
!insertmacro: end of SetSectionFlag
!insertmacro: _If
!insertmacro: end of _If
!insertmacro: ClearSectionFlag
Push: $R0
Push: $R1
Push: $R2
StrCpy $R2 "2" () ()
SectionGetFlags: $R2->$R0
IntOp: $R1=32~
IntOp: $R0=$R0&$R1
SectionSetFlags: $R2->$R0
Pop: $R2
Pop: $R1
Pop: $R0
!insertmacro: end of ClearSectionFlag
!insertmacro: _EndIf
!insertmacro: end of _EndIf
