Function: "PageReinstallModeUpdateSelection"
!insertmacro: __NSD_GetState
SendMessage:(->$ReinstallModePage.optBtn1.state)($ReinstallModePage.optBtn1,0x00F0,0,0)
!insertmacro: end of __NSD_GetState
!insertmacro: __NSD_GetState
SendMessage:(->$ReinstallModePage.optBtn2.state)($ReinstallModePage.optBtn2,0x00F0,0,0)
!insertmacro: end of __NSD_GetState
!insertmacro: _If
!insertmacro: end of _If
StrCpy $ReinstallMode "2" () ()
!insertmacro: _Else
!insertmacro: end of _Else
StrCpy $ReinstallMode "1" () ()
!insertmacro: _EndIf
!insertmacro: end of _EndIf
FunctionEnd
Function: "PageLeaveReinstallMode"
StrCpy $EXPRESS_UPDATE "0" () ()
!insertmacro: _If
!insertmacro: end of _If
!insertmacro: _And
!insertmacro: end of _And
StrCpy $EXPRESS_UPDATE "1" () ()
Call "RunUninstaller"
Quit
!insertmacro: _EndIf
!insertmacro: end of _EndIf
!insertmacro: _If
!insertmacro: end of _If
!insertmacro: _And
!insertmacro: end of _And
StrCpy $EXPRESS_UPDATE "1" () ()
Call "LoadPreviousSettings"
!insertmacro: _EndIf
!insertmacro: end of _EndIf
FunctionEnd
